{
    "compilerOptions": {
      "declaration":false,
      "emitDecoratorMetadata":true,
      "experimentalDecorators":true,
      "lib": [
        "es6",
        "dom"
      ],
      "module":"es6",
      "moduleResolution":"node",
      "sourceMap":true,
      "target":"es5",
      "jsx":"react",
      "allowSyntheticDefaultImports":true,
      "noImplicitAny":false,
      "noImplicitThis":true,
      "noImplicitReturns":true,
      "strictNullChecks":true,
      "types": [ "node" ],
      "typeRoots": [ "../node_modules/@types" ]
    },
    "exclude": [
      "**/*.spec.ts",
      "node_modules",
      "public",
      "vendor/bundle"
    ],
    "compileOnSave":false
  }